<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" percentHeight="5.0" vgrow="SOMETIMES" />
      <RowConstraints percentHeight="95.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <AnchorPane>
         <children>
            <HBox alignment="BOTTOM_LEFT" layoutX="5.0" layoutY="-4.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="7.0" AnchorPane.topAnchor="0.0">
               <children>
                   <TextField fx:id="filterTextField" layoutX="5.0" layoutY="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0">
                     <HBox.margin>
                        <Insets bottom="20.0" />
                     </HBox.margin></TextField>
                  <Button fx:id="searchButton" defaultButton="true" mnemonicParsing="false" onAction="#filter">
                     <HBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" />
                     </HBox.margin>
                     <graphic>
                        <ImageView fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../img/search.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Label fx:id="safeSearchLabel" text="Safe search:">
                     <HBox.margin>
                        <Insets bottom="30.0" />
                     </HBox.margin>
                     <padding>
                        <Insets left="20.0" right="5.0" />
                     </padding>
                  </Label>
                  <Slider fx:id="safeSearch" blockIncrement="5.0" minorTickCount="5" showTickLabels="true" showTickMarks="true" snapToTicks="true">
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor>
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" top="30.0" />
                     </HBox.margin>
                  </Slider>
                  <ProgressIndicator fx:id="progressIndicator" layoutX="108.0" layoutY="-4.0" minHeight="30.0" progress="0.0">
                     <HBox.margin>
                        <Insets bottom="20.0" left="5.0" top="15.0" />
                     </HBox.margin>
                     <padding>
                        <Insets left="20.0" />
                     </padding></ProgressIndicator>
               </children>
            </HBox>
             <Button fx:id="addButton" layoutX="166.0" layoutY="5.0" mnemonicParsing="false" onAction="#add" text="Add" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
         </children>
      </AnchorPane>
       <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" GridPane.rowIndex="1">
          <content>
             <TilePane fx:id="tilePane" hgap="5.0" prefColumns="9" prefRows="3" prefTileHeight="192.0" prefTileWidth="116.0" vgap="15.0">
                <padding>
                   <Insets left="5.0" top="10.0" />
                </padding>
             </TilePane>
          </content>
       </ScrollPane>
   </children>
</GridPane>
